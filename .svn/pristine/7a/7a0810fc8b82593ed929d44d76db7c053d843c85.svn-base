<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>filter()</title>
    <script src="http://www.anydo.wang/resource/js/jquery-3.1.1.js"></script>
    <style>
        .test1,.tst{
            width: 80%;
            margin:0 auto 50px;
            text-align: center;
        }
    </style>
</head>
<body>
<div class="test1">
    <li>1</li>
    <li class="test2">2</li>
    <li>3</li>
    <li id="tst">4</li>
    <li>5</li>
    <li>6</li>
</div>

<div class="tst">
    <li>1</li>
    <li class="tst1">2</li>
    <li>3</li>
    <li>4</li>
    <li>5</li>
    <li>6</li>
</div>
<script>
       var $Li=$('.test1 li');
       var $newLi=$Li.filter(function(i){
           alert(this.id);
            if(this.id=='tst'){
                return false;
            }
           else{
                return true;
            }
        });
       alert($newLi.length);


