*{
    margin: 0px;
    border: 0px;
    padding: 0px;
}
body{
    background-color: white;
}
ul{
    list-style: none;
}
.nav-wrap{
    width: 100%;
    background-color: white;
    height: 100px;
    box-shadow: 0px 4px 3px rgb(200,200,200);
}
.nav{
    width: 1200px;
    margin: 0 auto;
    height: 100px;
}
.nav img{
    margin-top: 15px;
}.nav ul{
    float: right;
     margin-top: 38px;
 }
 .nav ul li{
     display: inline;
     font-size: 21px;
     color: rgb(50, 50, 50);
     font-family: "Adobe 仿宋 Std R";
     margin-left: 60px;
 }
.nav ul li a{
    text-decoration: none;
    color:rgb(50, 50, 50);
}
.nav ul li a:hover{
    color: red;
    font-weight: bold;
}
.nav ul li a:visited{
    color: red;
}