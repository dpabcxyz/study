<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>yieled</title>
</head>
<body>
    <script>
        function* fn() {
            var x = 1;
            var y = yield(x+1);
            var z = yield(x+y);
            return z;
        }
        var a=fn();
        console.log(a.next()) //2
        console.log(a.next(2))//3
        console.log(a.next(5))//5
     </script>
</body>
</html>