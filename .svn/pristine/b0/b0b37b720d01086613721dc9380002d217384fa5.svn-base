<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<title>j结构赋值的应用1</title>
	</head>
	<body>
		<script>
			//多个参数
			function test1([a,b,c]=[1,1,1]){
				console.log(a+b+c);
			}
			test1([1,2,3]);
			
			//1个参数
			function test2(x=[1,2,3,]){
				var [a,b,c]=x;
				console.log(a);
			}
			test2([2,2,2])
		</script>
	</body>
</html>
