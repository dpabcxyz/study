<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>事件</title>
    <script src="../js/bower_components/react/react.js"></script>
    <script src="../js/bower_components/react/react-dom.js"></script>
    <script src="../js/bower_components/babel/browser.js"></script>
</head>
<body>
    <div id="frame"></div>
    <script type="text/babel">
    var ele=document.getElementById('frame');
    var A1=React.createClass({
        getInitialState:function(){
            return{
                val:'这是原内容'
            }
        },
        handelChange:function(){
            this.setState({
                val:'变化后内容！'
            })
        },
        render:function(){
            var val=this.state.val;
            return<div>
                <ul><button onClick={this.handelChange}>改变值</button></ul>
                <ul>{val}</ul>
                </div>
        }
    });
    ReactDOM.render(
        <A1/>,ele
    )
    </script>
</body>
</html>