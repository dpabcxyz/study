<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>v-else-if</title>
	 <script src="../js/VUE.js"></script>
</head>
<body>
	<div id="app">
		<div v-if="type === 'A'">A</div>
		<div v-else-if="type === 'B'">B</div>
		<div v-else-if="type === 'C'">C</div>
		<div v-else>Not A/B/C</div>
	</div>
</body>
<script type="text/javascript">
	var app=new Vue({
		el:'#app',
		data:{
			type:'A'
		}
	})
</script>
</html>