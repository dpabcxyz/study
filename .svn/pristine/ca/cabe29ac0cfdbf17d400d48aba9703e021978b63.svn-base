<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>stringify</title>
</head>

<body>
    <h2 id="test1"></h2>
    <H2 id="test2"></H2>
    <script>
        var obj = {
            "name": "Tome",
            "sex": "girl",
            "age": 18,
            "city": "北京",
        };
        var arr = ["Google", "Runoob", "Taobao", "Facebook"];
        var myObj1 = JSON.stringify(obj);
        var myObj2 = JSON.stringify(arr);
        document.getElementById('test1').innerHTML = myObj1;
        document.getElementById('test2').innerHTML = myObj2;
    </script>
</body>

</html>