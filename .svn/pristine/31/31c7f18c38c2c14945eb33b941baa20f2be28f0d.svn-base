<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>父子组件</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css.css">
    <script src="../js/VUE.js"></script>
</head>
<body >
	<div class="title">首页</div>
	<div id="box">
		<tabbar></tabbar>
	</div>
	
	<template id="t">
		<div class="tabbar">
			<item>
				<img src="../img/tabBar/baidu.png" slot="img">
				<span slot="txt">百度</span>
			</item>
			<item>
				<img src="../img/tabBar/google.png" slot="img">
				<span slot="txt">google</span>
			</item>
			<item>
				<img src="../img/tabBar/sougou.png" slot="img">
				<span slot="txt">搜狗</span>
			</item>
			<item>
				<img src="../img/tabBar/bing.png" slot="img">
				<span slot="txt">必应</span>
			</item>
			<item>
				<img src="../img/tabBar/qq.png" slot="img">
				<span slot="txt">腾讯</span>
			</item>
		</div>
	</template>

	<template id="child">
		<ul>
			<li class="symbol"><slot name="img"></slot></li>
			<li class="txt"><slot name="txt"></slot></li>
		</ul>
	</template>
</body>
<script type="text/javascript">
		var vm=new Vue({
			el:'#box',
			data:{
			},
			components:{
				'tabbar':{
					template:'#t',
					components:{
						'item':{
							template:'#child'
						}
					}
				}
			}
		})
</script>
</html>