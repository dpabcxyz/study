<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>函数队列</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.0/jquery.min.js"></script>
</head>
<body>
<button id="btn">action</button>
</body>
<script>
    $(function () {
        $('#btn').on('click',function () {
            $(document).dequeue('chenxuan');
        })
    });

// 创建函数队列
    $(document).queue('chenxuan',[
        function (next) {
        console.log(0);
            next();
        },
        function (next) {
            console.log(1);
            next();
        },
        function (next) {
            console.log(2);
            next();
        },
        function (next) {
            console.log(3);
            next();
        },
        function (next) {
            console.log(4);
            next();
        },
        function (next) {
            console.log(5);
            next();
        }
    ]);

    // 向函数队列添加函数，注意函数执行顺序
    $(document).queue('chenxuan',function () {
        console.log(999)
    })

</script>
</html>