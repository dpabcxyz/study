<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>全局设置</title>
    <script src="../js/jauery-3.2.1.js"></script>
    <link rel="stylesheet" href="../css/common.css">
</head>
<body>
<button class="button">开始</button>
<script>
    $(function () {
        $.ajaxSetup({
            timeout:5000
        });

        $('.button:eq(0)').click(function () {
            $.ajax({
                url:'setup.php',
                success:function (data,textStatus,jxhr) {
                    console.log(data);
                },
                gloal:false
            });
        });
        /*1当所有 AJAX 请求完成时*/
        $(document).ajaxComplete(function (event,jqxhr,ajaxOption) {
            alert(ajaxOption);
        });

        /*2当所有 AJAX 请求发生错误时*/
        $(document).ajaxError(function (event,jqxhr,ajaxOption,ThrownError) {
            alert(ajaxOption);
        });

        /*3请求即将发送*/
        $(document).ajaxSend(function (event,jqxhr,ajaxSetting,ajaxOption) {
            alert(ajaxOption);
        });

        /*4当 AJAX 请求开始时，显示“加载中”的指示*/
        $(document).ajaxStart(function () {
            alert("");
        });

        /*5当 AJAX 请求结束时*/
        $(document).ajaxStop(function () {
            alert("");
        });

       /*6当所有 AJAX 请求成功时*/
        $(document).ajaxSuccess(function (event,jqxhr,ajaxOption,data) {
            alert("");
        });
    })
</script>
</body>
</html>