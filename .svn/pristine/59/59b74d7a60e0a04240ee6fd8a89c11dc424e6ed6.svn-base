<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>hasOwnProperty()</title>
</head>
<body>
    <script>
        /*
        hasOwnProperty 返回一个布尔值，用于判断一个属性是本地属性还是从prototype继承过来的
        */
        function Person(name,height){
           this.name=name;
           this.height=height;
        }
        Person.prototype.sex=function(){
            return '男'
        }
        var boy=new Person('chenxuan',180);
        console.log(boy.hasOwnProperty('sex'));
        console.log(boy.hasOwnProperty('name'));
    </script>
</body>
</html>