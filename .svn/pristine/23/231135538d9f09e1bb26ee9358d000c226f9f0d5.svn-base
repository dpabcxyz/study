<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>
<script>
    var obj={
        set c(x){
            console.log('c被赋值：',x);
            c=x;
        },
        get c(){
            console.log('c被取出: ',c);
            return c
        }
    };

    obj.c=5  //c被赋值： 3
    obj.c  //c被取出:  3
</script>
