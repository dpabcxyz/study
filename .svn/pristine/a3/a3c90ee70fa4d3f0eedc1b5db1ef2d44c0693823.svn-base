<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>获取上一个兄弟元素</title>
    <script src="http://www.anydo.wang/resource/js/jquery-3.1.1.js"></script>
    <style>
        li{
            list-style: none;
        }
        .test1,.test2,.test3,.test4,.test5{
            width: 80%;
            margin: 100px auto 0;
            text-align: center;
        }
    </style>
</head>
<body>
<div class="test1">
    <ul>
        <li>1</li>
        <li>2</li>
        <li>3</li>
        <li>4</li>
        <li>5</li>
        <li class="last">6</li>
    </ul>
</div>

<div class="test2">
    <ul>
        <li class="one">1</li>
        <li>2</li>
        <li>3</li>
        <li>4</li>
        <li>5</li>
        <li>6</li>
        <li>7</li>
        <li>8</li>
        <li>9</li>
        <li class="ten">10</li>
    </ul>
</div>

<div class="test3">
    <ul>
        <li>1</li>
        <li>2</li>
        <li>3</li>
        <li>4</li>
        <li>5</li>
        <li>6</li>
        <li class="seven">7</li>
        <li >8</li>
        <li>9</li>
        <li>10</li>
    </ul>
</div>

<div class="test4">
    <ul>
        <li>1</li>
        <li>2</li>
        <li>3</li>
        <li>4</li>
        <li class="li">5</li>
        <li >6</li>
        <li>7</li>
        <li class="eight">8</li>
        <li>9</li>
        <li>10</li>
    </ul>
</div>
<script>
    /*prev(),获取紧挨着的上一个同辈元素*/
    $('.last').prev().css('color','red');

    /*prev(),获取紧挨着的上面的全部同辈元素*/
    $('.last').prevAll().css('font-size','30px');

    /*prev(),获取紧挨着的上面的全部同辈元素*/
    $('.ten').prevUntil('one').css('font-size','30px');

    /*prev(),获取紧挨着的上面的全部同辈元素*/
    $('.seven').siblings().css('color','green');
    $('.eight').siblings(":not('.li')").css('color','blue');






</script>
</body>
</html>