		<script>
			var btns=JSON.parse(sessionStorage.getItem('btns'))[window.location.pathname.replace(webName+'/', '')]
			 //遍历按钮权限决定现隐
			for(var i=0;i<btns.length;i++){
				if(btns[i]=="添加" || btns[i]=="修改" || btns[i]=="删除"){
					
				}
			}
			 var addFlag = btns.some(item=>{
			    if(item.menuName=='添加'){
			       return true
			   }
			 })
			 
			 var editFlag = btns.some(item=>{
