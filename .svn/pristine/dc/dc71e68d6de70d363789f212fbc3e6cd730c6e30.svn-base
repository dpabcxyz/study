<body>
	<script>
		var info1=[
		      {"name":"Jone","age":18},
			  {"name":"Amy","age":22},
			  {"name":"jay","age":10}
			  ];
	 	var info2=[
			      {"class":"6","num":01},
				  {"class":"4","num":02},
				  {"class":"5","num":03}
				  ];
		var newinfo=[];	  
		 for(i=0;i<info1.length;i++){
		 	 var newobj=Object.assign({},info1[i],info2[i]);//加{}不改变原对象（info1）
		 	newinfo.push(newobj);
		 }
		 console.log(info1);
		 console.log(newinfo);
	</script>
</body>
