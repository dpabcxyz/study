<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>v-text</title>
    <script src="../js/VUE.js"></script>
</head>
<body>
<div id="app" v-text="content">
    <h1>hello world!'</h1>
</div>
</body>
<script>
    var app=new Vue({
        el:'#app',
        data:{
            content:'hello world!'
        }
    })
</script>
</html>