<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>
<script>
    class Num {
        constructor(num) {
            this._num = num;
        }

        get num() {
            return this._num;
        }

        set num(num) {
            this._num = num;
        }

    }

    var test = new Num(9);
    console.log(test.num);
    test.num = 99;
    console.log(test.num);
</script>
</body>
</html>