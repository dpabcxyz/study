<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>属性表达式</title>
</head>
<body>
    <script>
        const methodsName="getArea";
        class Fn {
            constructor(){

            }
            name(){
                console.log('name')
            }
            [methodsName](){
                console.log("北京")
            }
        }
        new Fn().getArea();
    </script>
</body>
</html>