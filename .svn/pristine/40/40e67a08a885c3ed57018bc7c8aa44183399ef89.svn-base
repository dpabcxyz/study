        <li class="tab">#带阴影柱图#</li>
        <li class="tab">折线图1</li>
        <li class="tab">折线图2</li>
        <li class="tab">折线图3</li>
        <li class="tab">柱图</li>
        <li class="tab">柱图</li>
    
