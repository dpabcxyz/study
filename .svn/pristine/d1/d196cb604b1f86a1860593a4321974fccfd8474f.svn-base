<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>处理html</title>
    <script src="../../js/jauery-3.2.1.js"></script>
</head>
<body>
<button class="btn">ACTION</button>
<script>
    $('.btn').click(function () {
        var xhr=new XMLHttpRequest();
        xhr.addEventListener('readystatechange',function () {
            if (xhr.readyState==4){
                var obj=JSON.parse(xhr.responseText);
                var str='';
                for (var i=0;i<obj.length;i++) {
                    str+="<p><a href='"+obj[i].url+"'>"+obj[i].title+"</a></p>";
                }
                $('body').append(str);
            }
        });
        xhr.open('get','json.php');
        xhr.send(null);
    });
</script>

</body>
</html>