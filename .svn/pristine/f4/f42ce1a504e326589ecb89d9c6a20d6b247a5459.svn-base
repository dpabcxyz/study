<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>confirm()</title>
    <script src="http://www.anydo.wang/resource/js/jquery-3.1.1.js"></script>
</head>
<body>
<button class="btn1">confirm</button>
<button class="btn2">prompt</button>
<script>
    $(function(){
        $('.btn1').on('click',function(){
            var sure=confirm('你吃饭了吗？')
            if (sure==true){
                alert('吃饭了')
            }
            else{
                alert('没有吃饭')
            }
        })
    })
    $(function(){
        $('.btn2').on('click',function(){
           var soure=prompt('请输入您的姓名');
            if (soure==1){
                alert('hello!')
            }
            else {
                alert('哈哈哈')
            }
        })
    })
</script>
</body>
</html>